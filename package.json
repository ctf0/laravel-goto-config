{
    "name": "laravel-goto-config",
    "displayName": "Laravel Goto Config",
    "description": "Quick jump to config",
    "version": "0.2.8",
    "publisher": "ctf0",
    "engines": {
        "vscode": "^1.52.0"
    },
    "repository": "https://github.com/ctf0/laravel-goto-config",
    "icon": "images/logo.png",
    "categories": [
        "Other"
    ],
    "keywords": [
        "PHP",
        "Laravel",
        "goto",
        "file navigation"
    ],
    "activationEvents": [
        "onLanguage:php",
        "onLanguage:blade",
        "onUri",
        "onStartupFinished"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Laravel Goto Config",
            "properties": {
                "laravelGotoConfig.folders": {
                    "type": "array",
                    "default": [
                        "config"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "minItems": 1,
                    "uniqueItems": true,
                    "description": "where to search for config files"
                },
                "laravelGotoConfig.methods": {
                    "type": "array",
                    "default": [
                        "config(",
                        "config([",
                        "Config::get(",
                        "Config::set("
                    ],
                    "items": {
                        "type": "string"
                    },
                    "minItems": 1,
                    "uniqueItems": true,
                    "description": "methods to search for"
                },
                "laravelGotoConfig.forceShowConfigLink": {
                    "type": "boolean",
                    "default": true,
                    "description": "force add a fake link to config key even if we cant find the file (so we can view its value)"
                },
                "laravelGotoConfig.phpCommand": {
                    "type": "string",
                    "default": "php artisan",
                    "description": "the php command to use for running tinker (any quotes needs to be escaped)"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.23",
        "@types/vscode": "^1.52.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.2",
        "tslint": "^6.1.3",
        "typescript": "^4.6.3",
        "vscode-test": "^1.5.0"
    },
    "dependencies": {
        "await-exec": "^0.1.2",
        "escape-string-regexp": "^4.0.0",
        "fast-glob": "^3.2.11",
        "lodash": "^4.17.21"
    }
}
